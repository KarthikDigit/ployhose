<?xml version="1.0" encoding="utf-8"?>
<com.polyhose.customview.MultiStateView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/multiStateView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    app:msv_animateViewChanges="true"
    app:msv_emptyView="@layout/empty_view"
    app:msv_errorView="@layout/error_view"
    app:msv_loadingView="@layout/loading_view"
    app:msv_viewState="content"
    tools:context="com.polyhose.dashboard.anouncement.AnouncementFragment">

    <com.polyhose.customview.CustomMessageView
        android:id="@+id/anouncement_listview"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />


</com.polyhose.customview.MultiStateView>